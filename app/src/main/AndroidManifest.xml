<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GoFIT"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:exported="true"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity
            android:name=".SignupActivity"
            android:exported="true"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity
            android:name=".WelcomeActivity"
            android:exported="true"
            android:parentActivityName=".SignupActivity">
        </activity>
        <activity
            android:name=".GenderActivity"
            android:exported="true"
            android:parentActivityName=".WelcomeActivity">
        </activity>
        <activity
            android:name=".BirthdayActivity"
            android:exported="true"
            android:parentActivityName=".GenderActivity">
        </activity>
        <activity
            android:name=".HeightActivity"
            android:exported="true"
            android:parentActivityName=".BirthdayActivity">
        </activity>
        <activity
            android:name=".WeightActivity"
            android:exported="true"
            android:parentActivityName=".HeightActivity">
        </activity>
        <activity
            android:name=".GoalsActivity"
            android:exported="true"
            android:parentActivityName=".WeightActivity">
        </activity>
        <activity
            android:name=".TargetActivity"
            android:exported="true"
            android:parentActivityName=".GoalsActivity">
        </activity>
        <activity
            android:name=".LevelActivity"
            android:exported="true"
            android:parentActivityName=".TargetActivity">
        </activity>
        <activity
            android:name=".SignupSuccessActivity"
            android:exported="true"
            android:parentActivityName=".LevelActivity">
        </activity>
        <activity
            android:name=".HomeActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".WorkoutActivity"
            android:exported="true"
            android:parentActivityName=".HomeActivity">
        </activity>
        <activity
            android:name=".ExerciseActivity"
            android:exported="true"
            android:parentActivityName=".WorkoutActivity">
        </activity>
    </application>

</manifest>